{
  "name": "httpoz/hook",
  "description": "Package for allowing and monitoring a[pp access.",
  "keywords": ["laravel"],
  "homepage": "https://github.com/httpoz/hook",
  "license": "MIT",
  "authors": [{
    "name": "Oscar Mwanandimai",
    "role": "Architect"
  }],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1.3",
    "illuminate/support": "~5.0",
    "illuminate/database": "~5.0",
    "webpatser/laravel-uuid": "^3.0"
  },
  "require-dev": {
    "orchestra/database": "~3.6.0",
    "orchestra/testbench": "~3.6.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "HttpOz\\Hook\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HttpOz\\Hook\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "HttpOz\\Hook\\HookServiceProvider"
      ]
    }
  }
}
